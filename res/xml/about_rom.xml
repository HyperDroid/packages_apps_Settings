    <PreferenceCategory
        android:key="devs"
        android:title="@string/devs">

        <com.android.settings.widget.DeveloperPreference
           android:title="HyperDroidAOXP"
           slim:nameDev="HyperDroidAOXP"
           slim:twitterHandle="HyperDroidAOXP"
